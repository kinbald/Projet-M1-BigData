{% extends "base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('css/theme-product.css') }}" rel="stylesheet" />
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/item.js') }}"></script>
{% endblock %}

{% block body %}
{{ dump(test) }}
    <h1>Produits à insérer</h1>
    <table>
        {% for product in products %}
            <tr>
                {% for cell in product %}
                    <td style="border: 1px solid white; font-family:'Arial';">{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>

    <h1>Producteurs à insérer</h1>
    <table>
        {% for producer in producers %}
            <tr>
                {% for cell in producer %}
                    <td style="border: 1px solid white; font-family:'Arial';">{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>


    <h1>Médaille a insérer</h1>
    <table>
        {% for medal in medal %}
            <tr>
                {% for cell in medal %}
                    <td style="border: 1px solid white; font-family:'Arial';">{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>

    <h1>Tableau des expéditions</h1>
    <table>
        {% for shipment in shipments %}
            <tr>
                {% for cell in shipment %}
                    <td style="border: 1px solid white; font-family:'Arial';">{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>



    <h1>Tableau des options de livraison</h1>
    <table>
        {% for shipoption in shipoptions %}
            <tr>
                {% for cell in shipoption %}
                    <td style="border: 1px solid white; font-family:'Arial';">{{ cell }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
    </table>

{% endblock %}