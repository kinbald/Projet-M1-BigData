{% extends 'baseadmin.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('css/media.css') }}" rel="stylesheet" />
    <link href="{{ asset('css/table-template.css') }}" rel="stylesheet" />
{% endblock %}

{% block body %}
    <div class="container overflow-hidden">
        <div class="row">
            <div class="col-md-12">
                <h3 class="section-heading">{% trans %}Userwholesales list{% endtrans %}</h3>
            </div>
        </div>
    </div>

<div class="table-responsive table-template">
    <table class="table table-hover">
        <thead class="thead-default">
        <tr class="color-title">
            <th>Id</th>
            <th>{% trans %}Companyname{% endtrans %}</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for userWholesale in userWholesales %}
            <tr>
                <td><a href="{{ path('userwholesale_show', { 'id': userWholesale.id }) }}">{{ userWholesale.id }}</a>
                </td>
                <td>{{ userWholesale.companyName }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('userwholesale_show', { 'id': userWholesale.id }) }}">{% trans %}Show{% endtrans %}</a>
                        </li>
                        <li>
                            <a href="{{ path('userwholesale_edit', { 'id': userWholesale.id }) }}">{% trans %}Edit{% endtrans %}</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

    <ul>
        <li>
            <a href="{{ path('userwholesale_new') }}">
            <div class="btn btn-primary btn-circle">
                {% trans %}Create a new userWholesale{% endtrans %}
            </div>
            </a>
        </li>
    </ul>
{% endblock %}
