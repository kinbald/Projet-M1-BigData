{% extends 'base.html.twig' %}

{% block body %}
    <h1>{% trans %}Userproducer{% endtrans %}</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ userProducer.id }}</td>
        </tr>
        <tr>
            <th>Siret</th>
            <td>{{ userProducer.siret }}</td>
        </tr>
        <tr>
            <th>{% trans %}Billingemail{% endtrans %}</th>
            <td>{{ userProducer.billingEmail }}</td>
        </tr>
        <tr>
            <th>{% trans %}Website{% endtrans %}</th>
            <td>{{ userProducer.website }}</td>
        </tr>
        <tr>
            <th>Facebook</th>
            <td>{{ userProducer.facebook }}</td>
        </tr>
        <tr>
            <th>Twitter</th>
            <td>{{ userProducer.twitter }}</td>
        </tr>
        <tr>
            <th>{% trans %}Companyname{% endtrans %}</th>
            <td>{{ userProducer.companyName }}</td>
        </tr>
        <tr>
            <th>{% trans %}Businessname{% endtrans %}</th>
            <td>{{ userProducer.businessName }}</td>
        </tr>
        <tr>
            <th>{% trans %}Address{% endtrans %}</th>
            <td>{{ userProducer.address }}</td>
        </tr>
        <tr>
            <th>{% trans %}City{% endtrans %}</th>
            <td>{{ userProducer.city }}</td>
        </tr>
        <tr>
            <th>{% trans %}Country{% endtrans %}</th>
            <td>{{ userProducer.country }}</td>
        </tr>
        <tr>
            <th>{% trans %}Postal code{% endtrans %}</th>
            <td>{{ userProducer.postalCode }}</td>
        </tr>
        <tr>
            <th>{% trans %}State{% endtrans %}</th>
            <td>{{ userProducer.state }}</td>
        </tr>
        <tr>
            <th>Province</th>
            <td>{{ userProducer.province }}</td>
        </tr>
        <tr>
            <th>{% trans %}Phone{% endtrans %}</th>
            <td>{{ userProducer.phone }}</td>
        </tr>
        <tr>
            <th>Fax</th>
            <td>{{ userProducer.fax }}</td>
        </tr>
        <tr>
            <th>{% trans %}Tvaic{% endtrans %}</th>
            <td>{{ userProducer.tvaIC }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('userproducer_index') }}">{% trans %}Back to the list{% endtrans %}</a>
        </li>
        <li>
            <a href="{{ path('userproducer_edit', { 'id': userProducer.id }) }}">{% trans %}Edit{% endtrans %}</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="{% trans %}Delete{% endtrans %}">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
